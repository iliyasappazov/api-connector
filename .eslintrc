{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "rules": {
    "max-len": [
      "error",
      {
        "ignoreComments": true,
        "ignoreTemplateLiterals": true,
        "code": 120
      }
    ]
  }
}
